
    <section>
        <div class="container">
        <div class="row mt-4">
            {{#each products}}
            <div class="col-md-3 p-3">
                <div class="card" style="width: 15rem; height:30rem">
                    <img class="card-img-top"
                        src="/product-images/{{this.product._id}}.jpg"
                        alt="Card image cap" style="height:11rem">
                    <div class="card-body">
                        <h5 class="card-title">{{this.product.Brand}}</h5>
                        <h3 class="card-title">{{this.product.Model}}</h3>
                         <p class="card text">Price: â‚¹{{this.product.Price}}</p>
                         <p class="card-text">quantity:{{this.quantity}}</p>
                        <p class="card-text" >{{this.Description}}</p>
                        
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        
    </section>
